public class Main {
    public static void main(String[] args) {
    Cliente cliente1 = new Cliente();
    Trabajador trabajdor1 = new Trabajador();
    trabajdor1.edad = 22;
    trabajdor1.nombre = "Romer";
    trabajdor1.telefono = 699423852;
    trabajdor1.Salario = 1254;
    cliente1.edad = 18;
    cliente1.nombre = "Nestor";
    cliente1.telefono = 699529345;
    cliente1.credito = 5.34;

    System.out.println("El cliente se llama " + cliente1.nombre + " tiene la edad de " + cliente1.edad + " años, su numero telefonico es " + cliente1.telefono + " y su credito es de: " + cliente1.credito + "€");
    System.out.println("El trabajor se llama " + trabajdor1.nombre + " tiene la edad de " + trabajdor1.edad + " años, su numero telefonico es " + trabajdor1.telefono + " y su salario es de: " + trabajdor1.Salario + "€");
    }
}
class Persona{
    int edad;
    String nombre;
    int telefono;
}
class Cliente extends Persona{
    double credito;
}

class Trabajador extends Persona{
    int Salario;
}
